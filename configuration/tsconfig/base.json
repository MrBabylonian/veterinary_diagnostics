{
    "compilerOptions": {
        "target": "ES2023",
        "baseUrl": ".",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "forceConsistentCasingInFileNames": true,
        "allowJs": false,
        "skipLibCheck": true,
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "incremental": true
    }
}